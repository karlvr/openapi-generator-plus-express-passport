{{!--
Output a reference to a function to serialize the value to the form suitable for a response.
@param <this> CodegenSchema
@param prefix the import prefix for the functions
--}}
{{#if (isBoolean)}}
{{prefix}}booleanToJson
{{~else if (isInteger)~}}
{{prefix}}integerToJson
{{~else if (isNumber)~}}
{{prefix}}numberToJson
{{~else if (isString)~}}
{{prefix}}stringToJson
{{~else if (isBinary)~}}
{{prefix}}binaryToJson
{{~else if (isObject)~}}
{{prefix}}model{{className nativeType.parentType}}ToJson
{{~else if (isArray)~}}
{{prefix}}arrayToJson({{>frag/toResponse component.schema prefix=prefix}})
{{~else if (isEnum)~}}
{{prefix}}enum{{className nativeType.parentType}}ToJson
{{~else if (isDate)~}}
{{prefix}}dateToJson
{{~else if (isDateTime)~}}
{{prefix}}dateTimeToJson
{{~else if (isTime)~}}
{{prefix}}timeToJson
{{~else if (isMap)~}}
{{prefix}}mapToJson({{>frag/toResponse component.schema prefix=prefix}})
{{~else if (isInterface)~}}
{{prefix}}model{{className nativeType.parentType}}ToJson
{{~else if (isOneOf)~}}
{{prefix}}oneOf{{className nativeType.parentType}}ToJson
{{~else~}}
{{prefix}}unsupportedToJson
{{~/if}}